import java.util.Scanner;


public class p1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		 Scanner input=new Scanner(System.in);
	      int x=input.nextInt();
	      
	      System.out.println(x*x-3*x+7);
		
	}

}


